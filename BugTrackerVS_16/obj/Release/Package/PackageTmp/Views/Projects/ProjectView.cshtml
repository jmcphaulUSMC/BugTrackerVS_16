@model IEnumerable<BugTrackerVS_16.Models.ProjectsViewModel>
@{
    ViewBag.Title = "ProjectView";
}

<h2>Project Assigned to</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("User")
        </th>

    </tr>

    @foreach (var view in Model)
    {

        <tr>
            <td>
                    @view.projects.Name

            </td>

            <td>
               <p>@Html.ActionLink("Index", "Projects")" > Home</p>
            </td>
        </tr>

    }

</table>
